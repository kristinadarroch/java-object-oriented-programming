public class A1dot3 {
 
    public static void main(String[] args) {
 
        System.out.print("     J        A       V     V       A       \n");
        System.out.print("     J       A A       V   V       A A      \n");
        System.out.print(" J   J      AAAAA       V V       AAAAA     \n");
        System.out.print("  J J      A     A       V       A     A    \n");
 
    }
}
