// program displays 'Welcome to Java' five times
public class A1dot2 {
 
 public static void main(String[] args) {
 
  System.out.println("Welcome to Java");
  System.out.println("Welcome to Java");
  System.out.println("Welcome to Java");
  System.out.println("Welcome to Java");
  System.out.println("Welcome to Java");
 
 }
}
